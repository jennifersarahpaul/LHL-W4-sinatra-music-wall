<!-- THIS IS THE MUSIC WALL PAGE (LISTS ALL THE SONGS) -->

<a href="/messages/new">Now's your chance! Add a song to the wall!</a>
<hr/>

<% @messages.each do |message| %>
  <div class="message">
    <a href="/messages/<%= message.id %>"><h3><%= message.song_title %></a> <small>( by <%= message.artist %>)</small></h3>
  </div>
<% end %>